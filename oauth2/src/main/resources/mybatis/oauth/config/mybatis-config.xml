<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="lazyLoadingEnabled" value="true"/>
		<setting name="logImpl" value="LOG4J2"/>
	</settings>
	<typeAliases>
		<typeAlias alias="accessToken" type="com.idc.module.oauth.OauthAccessToken"/>
		<typeAlias alias="clientDetails" type="com.idc.module.oauth.OauthClientDetails"/>
		<typeAlias alias="refreshToken" type="com.idc.module.oauth.OauthRefreshToken"/>
	</typeAliases>
</configuration>  